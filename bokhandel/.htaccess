RewriteEngine On

RewriteCond %{THE_REQUEST} \s/index\.php[/\s] [NC]
RewriteRule ^ / [L,R=301]

ErrorDocument 404 /index.php

ErrorDocument 404 /index.php
ErrorDocument 403 /index.php
ErrorDocument 500 /index.php

<IfModule mod_expires.c>
    ExpiresActive On
    ExpiresDefault "access plus 1 month"
</IfModule>

