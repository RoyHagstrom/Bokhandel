RewriteEngine On

RewriteCond %{THE_REQUEST} ^[A-Z]{3,}\s/+(.+)\.php [NC]
RewriteRule ^/(.+?)\.php$ /$1 [R=301,L]
 
RewriteCond %{REQUEST_URI} ^.*index\.php(/.*)?$ [NC]
RewriteRule ^(.*)index\.php($|/) /$1 [R=301,L]
 
RewriteCond %{REQUEST_FILENAME}.php -f
RewriteRule ^(.+?)/?$ $1.php [L]
 
ErrorDocument 404 /index.php


